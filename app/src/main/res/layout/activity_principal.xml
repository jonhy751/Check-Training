<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.CheckTraining1.PrincipalActivity">


    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/bottomNavigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        app:mbn_backgroundBottomColor="@color/Menucolor"
        app:mbn_circleColor="@color/circleColor"
        app:mbn_countBackgroundColor="@color/countBackgoundColor"
        app:mbn_countTextColor="@color/countTextColor"
        app:mbn_defaultIconColor="@color/defaultIconColor"
        app:mbn_selectedIconColor="#000000"
        >

    </com.etebarian.meowbottomnavigation.MeowBottomNavigation>



    <ImageView
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:background="@drawable/cadas"
        android:layout_gravity="center|left"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="60dp"
        />
    <ImageView
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:background="@drawable/calend"
        android:layout_marginTop="60dp"
        android:layout_gravity="center|right"
        android:layout_marginRight="60dp"
        />
    <ImageView
        android:layout_width="411dp"
        android:layout_height="152dp"
        android:background="@drawable/cabe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"/>

    <TextView
        android:layout_width="206dp"
        android:layout_height="72dp"
        android:fontFamily="@font/coolveticarg"
        android:padding="10dp"
        android:shadowColor="@color/preto"
        android:shadowDx="2"
        android:shadowDy="3"
        android:shadowRadius="2"
        android:text="Menu"
        android:layout_gravity="top|center"
        android:textAlignment="center"
        android:layout_marginTop="100px"
        android:textColor="#FFFFFF"
        android:textSize="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <Button
        android:id="@+id/logout"
        android:layout_width="70dp"
        android:layout_height="33dp"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="200px"
        android:layout_marginRight="20dp"
        android:background="@drawable/botoes"
        android:text="Sair"
android:textColor="#FFFF"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.948"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.944" />

    <TextView
    android:id="@+id/textView7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Esse mes tem "
    android:fontFamily="@font/coolveticarg"
    android:textColor="@color/preto"
    android:shadowColor="@color/preto"
    android:shadowDx="2"
    android:shadowDy="3"
    android:shadowRadius="2"
    android:textSize="17dp"
    android:layout_gravity="center"
    android:layout_marginBottom="100dp"
        android:layout_marginRight="100dp"

    />
    <TextView
        android:id="@+id/textVieww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" X "
        android:fontFamily="@font/coolveticarg"
        android:textColor="@color/colorPrimary"
        android:shadowColor="@color/preto"
        android:shadowDx="2"
        android:shadowDy="3"
        android:shadowRadius="2"
        android:textSize="17dp"

        android:layout_gravity="center|left"
        android:layout_marginLeft="160dp"
        android:layout_marginBottom="100dp"


        />
    <TextView
        android:id="@+id/textViewww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Cadastrados"

        android:fontFamily="@font/coolveticarg"
        android:textColor="@color/preto"
        android:shadowColor="@color/preto"
        android:shadowDx="2"
        android:shadowDy="3"
        android:shadowRadius="2"
        android:textSize="17dp"
        android:layout_gravity="center|left"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="180dp"


        />


</FrameLayout>